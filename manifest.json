{
  "name": "test",
  "description": "study chrom extension",
  "version": "1.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "dog.png",
    "32": "dog.png",
    "48": "dog.png",
    "128": "dog.png"
  },
  "action": {
    "default_icon": "dog.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
